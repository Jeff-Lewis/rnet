<?xml version="1.0" encoding="utf-8" ?>

<configuration>

    <startup>
        <supportedRuntime
            version="v4.0"
            sku=".NETFramework,Version=v4.5" />
    </startup>

    <system.diagnostics>
        <sharedListeners>
            <add
                name="Console"
                type="System.Diagnostics.ConsoleTraceListener">
                <filter
                    type="System.Diagnostics.EventTypeFilter"
                    initializeData="Info" />
            </add>
        </sharedListeners>
        <trace>
            <listeners>
                <add
                    name="Console" />
            </listeners>
        </trace>
    </system.diagnostics>

</configuration>