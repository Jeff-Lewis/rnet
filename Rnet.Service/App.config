<?xml version="1.0" encoding="utf-8" ?>

<configuration>

    <startup>
        <supportedRuntime
            version="v4.0"
            sku=".NETFramework,Version=v4.5" />
    </startup>

    <system.serviceModel>
        <behaviors>
            <endpointBehaviors>
                <behavior>
                    <rnetWebHttp
                        automaticFormatSelectionEnabled="true"
                        defaultBodyStyle="Bare"
                        defaultOutgoingResponseFormat="Xml"
                        faultExceptionEnabled="true"
                        helpEnabled="true" />
                </behavior>
            </endpointBehaviors>
            <serviceBehaviors>
                <behavior>
                    <serviceMetadata
                        httpGetEnabled="true" />
                    <serviceDebug
                        includeExceptionDetailInFaults="true" />
                </behavior>
            </serviceBehaviors>
        </behaviors>
        <extensions>
            <behaviorExtensions>
                <add
                    name="rnetWebHttp"
                    type="Rnet.Service.RnetWebHttpElement, Rnet.Service"/>
            </behaviorExtensions>
        </extensions>
    </system.serviceModel>

    <system.diagnostics>
        <sources>
            <source
                name="System.ServiceModel"
                switchValue="Information, ActivityTracing"
                propagateActivity="true">
                <listeners>
                    <add
                        name="ServiceModelXmlWriterTraceListener"
                        type="System.Diagnostics.XmlWriterTraceListener"
                        initializeData= "Rnet.Service.svclog" />
                </listeners>
            </source>
        </sources>
    </system.diagnostics>

</configuration>